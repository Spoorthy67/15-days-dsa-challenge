class Solution {
    public boolean find132pattern(int[] nums) {
        int n=nums.length;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                for(int k=j+1;k<n;k++){
                    if(nums[i]<nums[k]&&nums[k]<nums[j]){
                        return true;
                    }
                }
            }
        }
        return false;
        
    }
}
