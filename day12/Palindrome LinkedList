class Solution {
    public boolean isPalindrome(ListNode head) {
        ArrayList<Integer> array=new ArrayList<>();
        while(head!=null){
            array.add(head.val);
            head=head.next;
        }
        int left=0;
        int right=array.size()-1;
        while(left<right){
            if(!array.get(left).equals(array.get(right))){
                return false;
            }
            left++;
            right--;
        }
        return true;

        
    }
}
