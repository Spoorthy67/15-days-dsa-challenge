class Solution {
    public boolean isAnagram(String s, String t) {
        if(s.length()!=t.length()){
            return false;

        }
        char[] ch1=s.toCharArray();
        char[] ch2=t.toCharArray();

        Arrays.sort(ch1);
        Arrays.sort(ch2);

        for(int i=0;i<s.length();i++){
            if(ch1[i]!=ch2[i]){
                return false;
            }
        }
        return true;
        
        
    }
}

Hashmap:

class Solution {
    public boolean isAnagram(String s, String t) {
        if(s.length()!=t.length()){
            return false;
        }
        HashMap<Character,Integer> map=new HashMap<>();
        for(int i=0;i<s.length();i++){
            char x=s.charAt(i);
            if(map.containsKey(x)){
                int existingVal=map.get(x);
                map.put(x,existingVal+1);

            }else{
                map.put(x,1);
            }
        }
        for(int i=0;i<t.length();i++){
            char x=t.charAt(i);
            if(map.containsKey(x)){
                int existingVal=map.get(x);
                map.put(x,existingVal-1);
            }else{
                map.put(x,-1);
            }
        }
        for(int i:map.values()){
            if(i!=0){
                return false;
            }
        }
        return true;
        
    }
}

